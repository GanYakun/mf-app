/* member/mine_infoshop/mine_infoshop.wxss */
.member-box{
  width: 100%;
  background-color: #f7f7f7;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  display: flex;
  flex-direction: column;
}

.top-box{
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
    position: absolute;
    width: 100%;
    height: 300rpx;
    top: -240rpx;
    display: flex;
    align-items: center;
    flex-direction: column;
    box-sizing: border-box;
    background-repeat:no-repeat;
}
.statistics-box{
  margin: 20rpx 20rpx 0 20rpx;
  padding: 42rpx;
  box-sizing: border-box;
  background-color: #ffffff;
  border-radius: 12rpx;
  position: relative;
  z-index: 9;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  font-size: 26rpx;
  color: #030303;
}
.statistics-item{
  width: 45%;
}
.statistics-item text{
  color: #FC8D34;
  margin-left: 50rpx;
}

.scrren-box{
  margin: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
  background-color: #ffffff;
  border-radius: 12rpx;
  position: relative;
  z-index: 9;
  display: flex;
  margin-top: 30rpx;
  flex-direction: column;
  justify-content: space-between;
}
.chioce-date{
  color: #595757;
  font-size: 28rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.icon-rili{
  color: 595757;
  margin-left: auto;
  font-size: 24rpx;
}

.reset{
	background-color: #eeeeee;
	border-radius: 5px;
  border: solid 1px #c9c9c9;
  margin-left: 52rpx;
  padding: 0 20rpx;
  font-size: 24rpx;
}

.time-connect{
  width: 30rpx;
  height: 2rpx;
  background-color: #898989;
  margin: 0 6rpx;
}

.time-box{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 55%;
}

.total{
  font-size: 28rpx;
  color: #595757;
}
.reset-ic{
  width: 34rpx;
  height: 34rpx;
  margin-left: auto;
}
.reset-ic image{
  display: block;
  height: 100%;
  width: 100%;
}
.num-box{
  display: flex;
  align-items: center;
  width: 35%;
  margin-left: auto;
}
.search-layout{
  height: 69rpx;
  margin: 0 20rpx 0 20rpx;
  display: flex;
  flex-direction: row;
  margin-bottom: 20rpx;
}
.search-layout .left{
  display: flex;
  flex-direction: row;
  flex: 1;
  background: #FFFFFF;
  border-radius: 9rpx;
  justify-content: space-between;
  align-items: center;
  padding-left: 18rpx;
  padding-right: 26rpx;
  box-sizing: border-box;
  font-size: 21rpx;
}
.search-layout .right{
  width: 183rpx;
  height: 69rpx;
  line-height: 69rpx;
  text-align: center;
  background: #F7E3D2;
  border-radius: 9rpx;
  color: #FE7B37;
  font-size: 21rpx;
  margin-left: 16rpx;
}
.member-list-head, .member-item{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  align-items: center;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  border-bottom: thin solid #ebebeb;
}

.member-item:last-child {
  border-bottom: none;
}

.member-list-head{
  background: #F8EEE6;
  color: #333333;
}
.member-list-head view{
  width: 20%;
  text-align: center;
}
.member-scroll{
  flex: 1;
  /* height: 150rpx; */
  overflow: hidden;
}
.member-item{
  background-color: white;
}
.member-scroll .text1{
  width: 20%;
  color: #555555;
  text-align: center;
}
.member-scroll .text2{
  width: 20%;
  color: #666666;
  text-align: center;
}
.divider{
  width: cala(100% - 54rpx);
  height: 1rpx;
  background: #EBEBEB;
  margin-left: 27rpx;
  margin-right: 27rpx;
}
.picker{
  font-size: 20rpx;
}