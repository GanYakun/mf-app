/* member/address_list/address_list.wxss */
page {
  background: #f1f1f1;
}


.van-swipe-cell__right {
  display: inline-block;
  width: 65px;
  height: 44px;
  font-size: 15px;
  line-height: 44px;
  color: #fff;
  text-align: center;
  background-color: #f44;
}
.main {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-bottom: 120rpx;
}

.item-view {
  width: 100%;
  background: #fff;
  padding: 20rpx;
  border-bottom: 1px solid #e5e5e5;
  background: #fff;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.mian-view {
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-right: 10rpx;

}

.phone-name {
  width: 100%;
  display: flex;
  flex-direction: row;
  font-size: 28rpx;
  color: #000;
  justify-content: space-between;
}

.address {
  font-size: 24rpx;
  color: #666;
  margin: 10rpx 0px;
}

.addressN {
  font-size: 24rpx;
  color: #999;
}

.btn-main {
  width: 100%;
  height: 100rpx;
  box-sizing: border-box;
  padding: 5px;
  background: #e7e7e7;
  position: fixed;
  bottom: 0;
}

.btn {
  height: 80rpx;
  /* border-radius: 80rpx; */
  background: #f97704;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
  margin-left: 20rpx;
  font-weight: bold;
}