.newflex-r-s{
  display: flex;
}

.content .item .img {
  width: 298rpx;
  /* width: 100%; */
  border-radius: 12rpx;
  display: flex;
  align-items: center;
}

.content .item .img img {
  height: 100%;
  width: 100%;
}

.content .item .titleclamp {
  margin-top: 10rpx;
  font-size: 28rpx;
  font-weight: bold;
  word-break: break-all;
    display: -webkit-box !important;
    -webkit-line-clamp: 2 !important;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.content .context {
  margin-top: 10rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  display: -webkit-box;
  font-size: 28rpx;
  -webkit-line-clamp: 2;
  /*这个数字是设置要显示省略号的行数*/
  -webkit-box-orient: vertical;
  white-space: normal;
  font-size: 22rpx;
  color: #666666;
  line-height: 35rpx;
}

.designcotent .title {
  color: #666666;
}

.designcotent .context {
  color: #888888;
  font-size: 12px;
  white-space: normal;
}

.designcotent .img {
  width: 31.3%;
  /* width:210rpx; */
  height: 140rpx;
  margin: 3px 1%;
}

.designcotent .img image {
  width: 100%;
  height: 100%;
  border-radius: 12rpx;
}

.designcotent .date {
  color: #d3d3d3;
  font-size: 12px;
}

.designcotent .replay {
  font-size: 12px;
  background: #ececec;
  padding: 10rpx;
  border-radius: 4rpx;
}

.designcotent .tipsinfo {
  align-items: center;
  flex-direction: row;
}

.dianzan {
  color: #F97704;
}

.nocolor {
  color: #E8E8E8;
}

.img image {
  border-radius: 12rpx;
}

.begood {
  font-size: 24rpx;
  white-space: normal;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.comprehensive {
  margin-top: 10rpx;
}
.department{
  color: #000;
  font-size:30rpx ;
}

.designcotent .item{
  width: 670rpx;
  padding: 10rpx 20rpx;
  margin: 0 auto;
  background: #ffffff;
  border-radius: 16rpx;
}