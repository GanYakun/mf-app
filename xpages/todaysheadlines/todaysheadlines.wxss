.box{
  width: 100%;
  padding: 20rpx;
  display: grid;
  grid-template-columns: 350rpx 350rpx;
  grid-column-gap: 20rpx;
  grid-row-gap: 20rpx;
  background-color: #ffffff;
}

.neibox image{
  width: 100%;
  height: 260rpx;
  border-radius: 12rpx;
}


.imagetitle{
  width: 355rpx;
  margin-top: 5rpx;
  text-align: center;
  font-size: 26rpx;
  overflow: hidden;
white-space: nowrap;
text-overflow: ellipsis; 
}
