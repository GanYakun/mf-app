/* pages/designsketch/designsketch.wxss */
@import "/utils/font.wxss";
@import "/utils/weui.wxss";
page{
  /* overflow: scroll; */
  background-color: #f7f7f7;
}
.headebox{
  background-color: #ffffff;

}
.heade{
  width: 93%;
  display: flex;
  justify-content: space-between;
  margin: 0 auto;
  height: 70rpx;
  align-items: center;
  }
  .headetitle{
   
    text-align: center;
  }
  
  .content{
/* display: grid;
grid-template-columns: 350rpx 350rpx;
grid-column-gap: 10rpx;
grid-row-gap: 10rpx; */
justify-content: center;
margin-top: 20rpx;
margin-bottom: 20rpx;
  }
  .mask{
    position:fixed;
    width:100%;
    height:100%;
    background: #363636;
    opacity: 0.5;
    z-index: 100;
  top: 0;
  }
  .Drawer{
    position:fixed;
    background:#eeeeee;
    width:300px;
    height:100%;
    right:-300px;
    z-index: 200;
  
    top: 0%;
  }
  .top_img{
    width:60px;
    height:60px;
    background:#000000;
  }
  .dirwerbox{
    height: 100rpx;
    background-color: #ffffff;
    margin-top: 1%;
    display: flex;
  align-items: center;
  
  
  }
  .drawertext,.drawercontent{
    flex-grow: 1;
    text-align: center;
  }
  .drawertext{
    font-size: 28rpx;
  }
  
  .drawertitle{
    font-size: 33rpx;
  }
  
  .choice{
    margin-top: 6%;
  }
  
  .listchioce{
    background-color: #ffffff;
    height: 90rpx;
  display: flex;
  align-items: center;
  margin-top: 1%;
  }
  
  .listchioceleft{
    margin-left: 30rpx;
  }
  
  .listchioceright{
    margin-left: auto;
    margin-right: 30rpx;
  }
  
  .detail{
    background-color: #ffffff;
    text-indent: 30rpx;
    height: 70rpx;
    line-height: 70rpx;
  }
  
  .cintentimg{
    background-color: #ffffff;
    border-radius: 12rpx;
    /* padding: 12rpx; */
    box-sizing: border-box;
    display: flex;
    padding: 20rpx;
    margin: 20rpx;


  }
  .beijintu{
    width: 300rpx;
    /* background:url(http://mf.100good.cn/plug-in/front/mlswap/images/loader.gif) no-repeat center center;background-size: contain; */
  
  }
  .beijintu image{
    /* width: 100%;
    height: 100%; */
    border-radius: 12rpx;
  }

  /* .cintentimg image{
  
  width: 330rpx;
  } */

  
  .introduce{
    font-size: 25rpx;
    font-weight: bold;
    margin-left: 2%;
    padding: 10rpx;
  }
  .context{
    padding: 25rpx 0;
    margin-left: 10rpx;
    font-size: 24rpx;
    width: 100%;
    color: #666666;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
  }
  .introducecontent{
    text-overflow:ellipsis;
    overflow:hidden;
    display:-webkit-box;
    font-size: 21rpx;
    -webkit-line-clamp:2; /*这个数字是设置要显示省略号的行数*/
    -webkit-box-orient:vertical;
    color: #666666;
    margin: 10rpx;
  }
  .inaddress{
    font-size: 25rpx;
    margin-bottom: 30rpx;
  }
  
  .textcolor{
    font-size: 30rpx;
    color: #000;
  }
  
  .frequency{
   
    font-size: 12px;
    color: #999999;
    display: flex;
    align-items: center;
  }

  .members{
    font-size: 32rpx;
    color: #3E3A39;
    font-weight: bold;
  }

  .city{
    color: #F97704;
    margin-left: 10rpx;
  }

  .ourteam{
    margin-left: 10rpx;
    color: #E9E9E9;
    font-size: 28rpx;

  }

  .titleshaixuan{
    font-size: 28rpx;
    color: #666666;
  }

  .icon-renqiiconx{
    font-size: 24rpx;
   color:#e82929;
  }
  .items{
    margin-bottom: 40rpx;
  }
  label{
    width: 30%;
    font-size: 12px;
    color: #666666;
  }
  .type {
    align-items: center;
    font-size: 12px;
    width: 70%;
    /* line-height: 60rpx;
    height: 60rpx; */
    border: 1px solid #ccc;
    padding: 0px 10px;
    box-sizing: border-box;
    border-radius: 8rpx;
  }
  .name{
    text-align: left;
    font-size: 12px;
    width: 70%;
    line-height: 60rpx;
    height: 60rpx;
    border: 1px solid #ccc;
    padding: 2px 10px;
    box-sizing: border-box;
    border-radius: 8rpx;
  }
  
.options {
  position: absolute;
  margin-left: 28%;
  width: 66%;
    border: 1px solid #ccc;
    box-sizing: border-box;
    margin-bottom: 20rpx;
    font-size: 14px;
    background-color: #fff;
    z-index: 999;
    height: 300rpx;
    overflow-y: auto;
  }
  .options1{
    position: absolute;
    margin-left: 28%;
    width: 66%;
      border: 1px solid #ccc;
      box-sizing: border-box;
      margin-bottom: 20rpx;
      font-size: 14px;
      background-color: #fff;
      z-index: 999;
      height: 150rpx;
      overflow-y: auto;
  }
  
.options .select {
    line-height: 60rpx;
    height: 60rpx;
  }
  
.options .selected {
    background-color: rgb(166, 210, 235);
    color: #fff;
  
  }
.weui-dialog{
  padding: 20rpx;
}
.queding{
  width: 260rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 16px;
    display: block;
    border-radius: 4rpx;
    background: #f97704;
    margin: 20rpx auto 0 auto;
    color: #fff;
}

.h5-box {
  width: calc(100% - 40rpx);
  margin-left: 20rpx;
  margin-top: 20rpx;
  /* padding: 20rpx; */
  box-sizing: border-box;
  /* background: #fff; */
  border-radius: 12rpx;
  overflow: hidden;
}

@import '../../wxParse//wxParse.wxss';

.wxParse-img{
  display: flex; 
  margin-top:-5rpx; /*解决图片空白间隙*/ 
  width: 100% !important;
  }
    .wxParse-p{
      font-size: 0;
    }
 