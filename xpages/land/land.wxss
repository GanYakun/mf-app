@import '../../wxParse/wxParse.wxss';

page {
  background-color: #f7f7f7;
}

.lunboswiper {
  height: 500rpx;
  width:100%;
  border-radius: 16rpx;
  overflow: hidden;
}

.notan {
  font-size: 28rpx;
  color: #000;
  width: 95%;
  margin: 0 auto;
  padding-top: 20rpx;
}

.transferContent {
  /* background-color: #ffffff; */
  width: 100%;
}

.caseIdea {
  font-size: 24rpx;
  color: #666666;
  width: 95%;
  margin: 0 auto;
  margin-top: 20rpx;
  white-space: pre-wrap;
  
}

.checkboxes {
  display: grid;
  grid-template-columns: 20% 20% 20% 20%;
  grid-column-gap: 10rpx;
  width: 95%;
  margin: 0 auto;
  margin-top: 20rpx;
}

.icon-xuanzhong {
  color: #F97704;
  font-size: 24rpx;
  font-weight: bold;
  margin-right: 10rpx;
}

.hasboxedvalue {
  font-size: 12px;
  color: #555;
}

.mapps {
  width: 95%;
  margin: 0 auto;
  margin-top: 20rpx;
}


.huadong {
  display: flex;
  background-color: #ffffff;
  padding-left: 20rpx;
  margin-top: 20rpx;
}

.huadong>view:first-child {
  width: 220rpx;
  /* height: 210rpx; */
}

.jiaone {
  color: #ddd;
  font-weight: bold;
  font-size: 70rpx;
  height: 150rpx;
  line-height: 150rpx;
}

.bootmtou {
  width: 220rpx;
  opacity: 0.8;
  color: #555555;
  line-height: 60rpx;
  font-size: 11px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.scroll_header {
  display: flex;
  white-space: nowrap;
  width: 60%;
  height: 210rpx;
}

.scroll_item:first-child {
  margin-left: 0;
}

.scroll_item {
  margin-left: 20rpx;
  display: inline-block;
  width: 220rpx;
  height: 150rpx;
}

.eclipsedesignerinstallation {
  border-bottom: solid 1px #e5e5e5;
  background-color: #fff;
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
  color: #555555;
  text-indent: 2.5%;
  width: 100%;
  margin: 0 auto;
  margin-top: 20rpx;
}

.professional {
  background-color: #ffffff;
  margin-top: 20rpx;
}

.show_elided {
  padding: 20rpx;
  display: flex;
  flex-direction: column;

}
.pictures_dss {
  height: 100%;
  width: 150rpx;
  border-bottom: solid 2px #f97704;
  color: #f97704;
  text-indent: 18.75rpx;
}

.bootmtwo {
  width: 100%;
  background-color: #ffffff;
  display: flex;
  justify-content: space-between;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #333;
}

.bootmtwo>view {
  text-align: center;
}

.bootmtwo>view:first-child {
  border-right: 1px solid rgba(0, 0, 0, .05);
  width: 30%;
}
.bootmtwo>view:last-child{
  width: 70%;
}
.collection{
  color: #f97704;
}
.dialog .info {
  display: flex;
  justify-content: center;
  margin: 30rpx 0;

}


.dialog {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  border-radius: 6rpx;
  padding: 20rpx;
  position: fixed;
  z-index: 5000;
  top: 50%;
  left: 9px;
right: 9px;
  /* -webkit-transform: translateY(-50%); */
  transform: translateY(-50%);
  background-color: #fff;
  width: 90%;
margin: 0 auto;

/* min-width: 260px; */
}

.share{
  width: 30%;
  border-right: 1px solid rgba(0, 0, 0, .05);
}

.TypeLevelPopularity {
  font-size: 23rpx;
  color: #666666;
}

.imagesControllertest {
  width: 398rpx;
  overflow: hidden;
}



.content-grids {
  /* padding-bottom: 40rpx; */
  background-color: #fff;
  width: calc(100% - 80rpx);
  display: flex;
  margin: 20rpx auto;
  padding: 20rpx;
  border-radius: 6rpx;
  align-items: center;

}



.containsRights {
  padding: 25rpx 0;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size: 26rpx;
  margin-left: 20rpx;
  width: 55%;
}
.alignment {
  color: #888888;
  margin-bottom: 34rpx;
}


.designerName {
  font-family: SourceHanSansCN-Medium;
  font-size: 28rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #222222;
}

.imagesControllertest image {
  border-radius: 16rpx;
  display: block;
}

.notan-box{
  width: 100%;
  background-color: #fff;
}


.top-plate{
  width: calc(100% - 40rpx);
  margin: 20rpx auto;
  padding: 20rpx;
  box-sizing: border-box;
  background-color: #fff;
}

.switch-options{
  display: flex;
  justify-content: center;
  margin-top: 20rpx;
}

.switch-options>view{
  margin-right: 20rpx;
  font-size: 26rpx;
  padding: 0 40rpx;
  text-align: center;
  color: #0090a2;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 60rpx;
  background-color: #e1eff1;
  opacity: 0.8;
  border: 2rpx solid #e1eff1;

}
.yes-options{
  background-color: #fff !important;
}
.video-box,.vr-box{
  width: 100%;
  height: 500rpx;
  border-radius: 16rpx;
  overflow: hidden;
}
.vr-box{
  background-size: cover;
}
.vr-icon{
  height: 100%;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #00000080;
}

.icon-VR1 {
  font-size: 80rpx;
  color: #fff;
}

.wxParse-p image{
  display: block;
}
.wx-banner-List image{
  width: 100%;
  margin-top: 10rpx;
  display: block;
}