.optname-budgets:before{
  display: block;
    content: '';
    position: absolute;
    left: 50%;
    top: 50%;
    width: 616rpx;
    height: 442rpx;
    margin: -220rpx 0 0 -308rpx;
    /* -webkit-animation: roateBg 40000ms infinite linear;
    animation: roateBg 40000ms infinite linear; */
    animation-name: myfirst; /*动画的名称 */
    animation-duration: 40000ms; /*动画从开始到结束的时间*/
    animation-timing-function: linear; /*动画执行快慢的参数*/
    animation-iteration-count: infinite; /*动画执行多少次的参数*//*以下是兼容ios所需，参数意义与上相同*/
    -webkit-animation-name: myfirst;
    -webkit-animation-duration: 40000ms;
    -webkit-animation-timing-function: linear;
    -webkit-animation-iteration-count: infinite;
}

@keyframes myfirst {
  /*开始转的角度*/
  from {
    transform: rotate(0deg);
  }/*结束的角度*/
  to {
    transform: rotate(360deg);
  }
}
/*兼容ios*/
@-webkit-keyframes myfirst {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(360deg);
  }
}

