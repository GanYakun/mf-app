.select{
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  width: 25%;
  /* border-bottom: 2rpx solid #f97704; */
  color: #f97704;
  line-height: 90rpx;
  text-align: center;
}
.tabitem{
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  width: 25%;
  line-height: 90rpx;
  height: 90rpx;
  text-align: center;
}

.line{
  width: 100rpx;
  height: 2rpx;
  background: #f97704;
}
.personnelbox{
  display: grid;
  grid-template-columns: auto auto;
  /* grid-template-rows: 630rpx 630rpx; */
  /* grid-column-gap:20rpx; */
  grid-row-gap: 10rpx;
  grid-column-gap: 10rpx;
  width: 720rpx;
  padding: 12rpx 0 0 0;
  margin: 0 auto;
}
.personnel{
  border-radius: 12rpx;
  width: 330rpx;
display: grid;
padding: 12rpx;
background-color: #ffffff;

}

.personnelview{
  /* background-color: #f97704; */
  /* width: 318rpx; */
  height: 100%;
}

.icon-xingxing1{
  color: #FFE648;
}

.bootmage{
  display: flex;
  justify-content: space-between;
  margin-top: 10rpx;
}
.bootmage view{
font-size: 24rpx;
color: #666666;
}
