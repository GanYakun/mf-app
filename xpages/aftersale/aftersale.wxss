.tabbox{
  height: 90rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  border-bottom: 1px solid #f2f2f2;
  background-color: #ffffff;
  padding: 0 20rpx;
}

