@import "../../../utils/font.wxss";
page{
  background: #f7f7f7;
}
.boxView{
  background: #ffffff;
}

.boxView>view{
padding: 40rpx 30rpx;
font-size: 28rpx;
}


.pickerView{
padding: 40rpx 30rpx;
font-size: 28rpx;
  border-top: 1rpx #f1f1f1 solid;
  border-bottom: 1rpx #f1f1f1 solid;
  display: flex;
    justify-content: space-between;
    align-items: center;
  }


.bootomView{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  z-index: 9;
  bottom: 0;
  height: 120rpx;
  background: #ffffff;
  box-shadow: 0px 10rpx 30rpx 0px rgba(138, 138, 138, 0.24);
}
.bootomView>view{
  padding: 20rpx  120rpx;
  font-size: 26rpx;
border-radius: 6rpx;
}
.bootomView>view:nth-child(1){
  background-color: #f7f7f7;
  margin-right: 40rpx;
}
.bootomView>view:nth-child(2){
  background-color: #ff6c00;
  color: #ffffff;
}
.startPicker{
  position: absolute;
  width: 100%;
  z-index: 9;
  height: 80rpx;
  background-color: #ff6c00;

}