page{
  background: #f2f2f2;
}

.topBox{
background-image: url(data:image/png;base64,);
width: 100%;
height: 383rpx;
background-size: 100%;
}

.hedimg{
  width:50%;
  margin: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100%;
}

.hedimg text{
  margin-top: 10rpx;
font-size: 28rpx;
color: #ffffff;
}

.hedimg image{
  width: 132rpx;
	height: 132rpx;
  border: solid 4rpx #ffffff;
  border-radius: 132rpx;
}

.contentMessage{
  background: #ffffff;
  width: calc(100% - 80rpx);
  margin: 0 auto;
  margin-top: 30rpx;
  padding: 30rpx 20rpx;
  border-radius: 12rpx;
}

.contentMessage>view{
  color: #545153;
  font-size: 28rpx;
  margin-top: 20rpx;
  display: flex;
  align-items: center;
  
}
.contentMessage>view{
  padding: 30rpx 0;
  border-bottom: 1rpx solid #ededed;
}

.contentMessage view input{
  margin-left: auto;
  text-align: right;
}

.icon-youjiantou{
  color: #999999;
}


.picker{
  margin-left: auto;
}

.icon-jia{
  margin-left: auto;
  font-size: 28rpx;
}

.personal-profile{
  display: flex;
  flex-direction: column;
  border-bottom:0rpx #ffffff solid !important;
}

.personal-profile>textarea{
background: #f9f9f9;
border-radius: 12rpx;
width: 100%;
margin-top: 10rpx;
min-height: 100rpx;
}

.submit{
  width: 680rpx;
	height: 98rpx;
	background-color: #f87a1b;
	box-shadow: -4rpx 3rpx 9rpx 0rpx 
		rgba(166, 71, 0, 0.31);
  border-radius: 49rpx;
  left: calc((100% - 680rpx)/2);
  color: #ffffff;
  text-align: center;
  line-height: 98rpx;
  position: fixed;
  bottom: 30rpx;
  z-index: 99;
}
.thumbnail{
  margin-left: auto;
}
.thumbnail image{
  width: 80rpx;
  height: 0rpx;
}

.icon-lajitong{
  color: #ff4e29;
  font-size: 28rpx;
  margin-left: 10rpx;
}

.profile-box{
  text-align: center;width:100%;display: block;margin-top:unset;
  border-bottom: thin solid #ededed;
  padding-bottom: 20rpx;
}
.video-upload,.image-upload{
  width: 100%;
  margin-top: 20rpx;
}

.video-title,.image-title{
  display: flex;
  align-items: center;
  width: 100%;
  justify-content: space-between;
}
.up-video{
  display: flex;
  width: 100%;
}
.flex-c{
  display: flex;
  flex-direction: row;
}
.up-video>video{
  width: 50%;
}
.up-video>view{
  position: relative;
}
.up-video>view>text{
  position: absolute;
  bottom: 0;
  left: 30rpx;
}
.video-introduce{
  background-color: #f3f3f3;
  padding: 20rpx;
  margin-top: 30rpx;
  box-sizing: border-box;
  height: 240rpx;
}

.video-introduce>textarea{
  color: #9fa0a0;
  font-size: 30rpx;
  width: 100%;
  height: 100%;
}
.image-video{
  display: grid;
  grid-template-columns: repeat(3,calc(33.33% - 15rpx)) ;
  grid-gap: 20rpx;
  margin-top: 20rpx;
}
.image-video>view{
  width: 100%;
  padding-top: 100%;
  position: relative;
}

.image-video>view image{
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;


}
.video-delete{
  font-size: 46rpx;
}

.move-box{
  position: absolute;
  z-index: 9;
  width: 100%;
  height: 26%;
  bottom: 0;
  display: flex;
  padding: 0 10rpx;
  box-sizing: border-box;
  justify-content: space-between;
  align-items: center;
  background-color: hsl(0, 0%, 0% , 0.2);
}

.delete-image{
  color: #ffffff;
}
.move-box>view{
  color: #ffffff;
  height: max-content;
  font-size: 35rpx;
}
.right-fanhui{
  transform: rotate(180deg);
}