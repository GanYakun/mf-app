.slide{
  width: 100%;
  transition: 1s;
  overflow: hidden;
}
.slide-image{
  border-radius: 16rpx;
}

.swiperimage{
  position: relative;
}
.image_count{
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
  border-radius: 30rpx;
  color: #ffffff;
  text-align: center;
  font-size: 24rpx;
  position: absolute;
  right: 2%;
  bottom: 15rpx;
}