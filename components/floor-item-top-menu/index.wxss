/* components/floor-item-top-menu/index.wxss */
@import "/utils/font.wxss";
.menuId {
  width: 100%;
  height: 155rpx;
  z-index: 9999;
  background: #ffffff;
  /* top: 150rpx; */
  /* margin-bottom: -20rpx; */
  /* padding-bottom: 5rpx; */
}

.swiper-dots {
  width: 60%;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  padding: 15rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  letter-spacing: 40rpx;
  box-sizing: border-box;
  margin: 0 auto;
}

.swiper-dot {
  width: 20rpx;
  height: 6rpx;
  background: #ebd498;
  border-radius: 6rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
  box-sizing: border-box;
}

.swiper-dot-active {
  width: 20rpx;
  height: 6rpx;
  background: #c8af7a;
  /* background-color: #475153; */
  border-radius: 6rpx;
  box-sizing: border-box;
}

.popupmenubox {
  width: calc(100% - 40rpx);
  /* height: 100%; */
  display: grid;
  margin: 0 auto;
  grid-template-columns: repeat(5,calc(25% - 45rpx)) ;
  grid-column-gap: 10rpx;
  /* grid-row-gap: 10rpx; */
  text-align: center;
  color: #666666;
  /* background-color: #ffffff; */
}

.case{
	width:60rpx;
	height:40rpx;
  background-image: url(data:image/png;base64,);
  position:absolute;
  margin-top: -13rpx;
  margin-left: 75rpx;
  animation: case  1000ms infinite;
  background-size: 100% 100%;
  transform-origin: 50% 100%;
 }

 @-webkit-keyframes case {
	0%, 100% {
		transform:rotate(0deg) scale(.95)
	}
	15%{
		transform:rotate(-6deg) scale(1)
	}
}
@keyframes case {
	0%, 100% {
		transform:rotate(0deg) scale(.95);
	}
	15%{
		transform:rotate(-6deg) scale(1);
	}
}

.imgmenu {
  display: flex;
  flex-direction: column;
  font-size: 28rpx;
  justify-content: center;
  color: #333;
}

.imgmenu image {
  /* padding: 10rpx 0; */
  width: 44rpx;
  height: 44rpx;
}

.imgmenu text {
  font-size: 24rpx;
  height: 40rpx;
  line-height: 40rpx;
  border-radius: 40rpx;
  padding: 0 10rpx;
  white-space: nowrap;
  /* background-color: #475153; */
  background: linear-gradient(to bottom, #f5eab9 , #dcb66b);
  color: #000000;
}
.imgmenu view{
  height: 20rpx;
  width: 100%;
}
.close{
  position: absolute;
  bottom: 0;
  right: 20rpx;
  z-index: 9;
}

.icon-cha,.icon-xiangxiayuanjiantouxiajiantouxianxing{
    color: #FE6E02;
    font-size: 22rpx;
    /* margin-bottom: -10rpx; */
}
.hiddeMenu{
  height: 50rpx !important;

}