/* components/furniture_tabbar/furniture_tabbar.wxss */
@import "/app.wxss";
.mobileFooter {
  color: #666666;
  box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.1);
  background-color: #fff;
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 92rpx;
  z-index: 1;
}

.mobileFooter .footerList .item {
  text-align: center;
  width: 45%;
  margin: 10rpx 0;
  font-size: 12px;
}

.mobileFooter .footerList .item .img {
  width: 42rpx;
  height: 42rpx;
  margin: 0 auto;
  margin-bottom: 4rpx;
}

.mobileFooter .footerList .item .img image {
  width: 100%;
  height: 100%;
}