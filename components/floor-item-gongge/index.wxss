/* components/floor-item/index.wxss */
.floor-item {
  /* background-color: #ffffff; */
  width: 100%;
  margin: 0 auto;
  overflow: hidden;
}

.titles {
  display: flex;
  width: 100%;
  align-items: baseline;
 
}

.title {
  font-size: 34rpx;
  font-weight: bold;
  color: #000000;
}

.sub-title {
  font-size: 24rpx;
  margin-left: 30rpx;
  color: #666666;
}

.more{
  font-size: 28rpx;
  color: #000000;
  margin-left: auto;
}

.build-list {
  width: 100%;
  white-space: nowrap;
  padding-top: 20rpx;
}

.build-item {
  color: #333;
  font-size: 26rpx;
  display: inline-block;
  padding-left: 40rpx;
  padding-right: 40rpx;
  box-sizing: border-box;
  border-right: 1px #333 solid;
  line-height: 26rpx;
}

.build-item:first-child {
  padding-left: 0;
}

.build-item:last-child {
  padding-right: 0;
  border-right: none;
}

.filter-list, .filter-list1, .filter-list2 {
  width: 100%;
  white-space: nowrap;
  padding-top: 20rpx;
}

.filter-list .filter-item {
  display: inline-block;
  width: 17%;
  margin-right: 1%;
  text-align: center;
  border-radius: 130rpx;
  font-size: 24rpx;
  color: #444;
}

.filter-list .filter-item:last-child {
  margin-right: 0;
}

.filter-list1 .filter-item {
  display: inline-block;
  height: 130rpx;
  width: 130rpx;
  line-height: 130rpx;
  text-align: center;
  border-radius: 130rpx;
  color: #ffffff;
  font-size: 26rpx;
  border: #f7f7f7 5rpx solid; 
  margin-left: 10rpx;
  margin-right: 10rpx;
}

.filter-list1 .filter-item:first-child {
  margin-left: 0;
}

.filter-list1 .filter-item:last-child {
  margin-right: 0;
}

.filter-list2 .filter-item {
  display: inline-block;
  min-width: 25%;
  /* flex-direction: row;
  align-items: center;
  justify-content: center; */
  color: #444;
  font-size: 26rpx;
  text-align: center;
}
.filter-list2 .filter-item:first-child {
  justify-content: flex-start;
}

.filter-list2 .filter-item:last-child {
  justify-content: flex-end;
}

.cauMask{
  width: 130rpx;
  height: 130rpx;
  position: absolute;
  border-radius: 130rpx;
  margin-top: 0;
  background-color: hsl(0, 0%, 20%,0.5);
  text-align: center;
  line-height: 130rpx;
}

.banner-list {
  width: 100%;
  padding-top: 20rpx;
}

.ad-image-list {
  width: 100%;
  display: flex;
  flex-direction: row;
  /* align-items: center; */
  justify-content: space-between;
  flex-wrap: wrap;
  /* background: #ffffff; */
  border-radius: 12rpx;
  /* padding: 0 10rpx; */
  box-sizing: border-box;
}

.ad-image-full-item {
  position: relative;
  width: 100%;
  border-radius: 12rpx;
  box-sizing: border-box;
  overflow: hidden;
  margin-top: 20rpx;
  background: #fff;
}

.price-image{
  position: absolute;
  right: 0;
  width: 240rpx;
  margin-top: 40rpx;
  height: 50rpx;
  background-color: #ffffff;
  border-top-left-radius: 50rpx;
  border-bottom-left-radius: 50rpx;
  z-index: 3;
  display: flex;
  align-items: center;
  padding-left: 5rpx;
}
.price-image text{
  margin-left: 20rpx;
  color: #666;
  font-size: 28rpx;
}

.ad-image {
  width: 100%;
  border-radius: 12rpx;
}

.ad-title {
  position: absolute;
  width: 100%;
  padding: 0 20rpx 10rpx 20rpx;
  margin-top: -10%;
  height: 70rpx;
  font-size: 26rpx;
  color: #fff;
  display: flex;
  align-items: flex-end;
  background:  -webkit-linear-gradient(bottom,#333333, hsla(0, 0%, 0%, 0.0));
  box-sizing: border-box;
}

.ad-title view {
  width: 100%;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.ad-desc {
  width: calc(100% - 40rpx);
  padding: 0 20rpx;
  background-color: #ffffff;
  white-space: pre-wrap;
  margin-top: -10rpx;
  line-height: 40rpx;
}

.ad-image-half-item {
  width: calc(50% - 7rpx);
  border-radius: 12rpx;
  box-sizing: border-box;
  overflow: hidden;
  margin-top: 14rpx;
  background: #fff;
}

.ad-image-half-item:nth-child(odd) {
  margin-right: 0rpx;
}

.ad-image-half-item:nth-child(even) {
  margin-left: 0rpx;
}
.ad-image-half-item:nth-child(1) {
  margin-top: 0rpx !important; 
}
.ad-image-half-item:nth-child(2) {
  margin-top: 0rpx !important; 
}

.ad-image-half-item .ad-image {
  width: 100%;
  height: 260rpx;
  display: block;
}
image {
  display:block;
}

.ad-image-wrapper{
  position: relative;
  width: 100%;
  /* background: #000000; */
}

.hottype {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  animation: hottype 1000ms infinite;
  width: 18%;
  height: 26%;
  position: absolute;
  left: 80%;
  right: 0;
  top: 10rpx;
  color: #fff;
  font-size: 24rpx;
  display:flex ;
  justify-content: center;
  align-items: center;
}

.activity-time-right{
  margin-left: 20rpx;
  font-size: 28rpx;
  color: #FE6E02;
}
.loader-inner{
  display: flex;
  align-items: center;
  justify-content: center;
}

.ball-pulse> text{
  margin-left: 20rpx;
  color: #f97004;
}

.ball-pulse > view {
  background-color: #f97004;
  width: 16rpx;
  height: 16rpx;
  border-radius: 100%;
  margin: 2px;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
  display: inline-block;
  
}
.ball-pulse > view:nth-child(1) {
  -webkit-animation: scale 0.75s 0.12s infinite cubic-bezier(.2, .68, .18, 1.08);
  animation: scale 0.75s 0.12s infinite cubic-bezier(.2, .68, .18, 1.08);
}
.ball-pulse > view:nth-child(2) {
  -webkit-animation: scale 0.75s 0.24s infinite cubic-bezier(.2, .68, .18, 1.08);
  animation: scale 0.75s 0.24s infinite cubic-bezier(.2, .68, .18, 1.08);
  }
.ball-pulse > view:nth-child(3) {
  -webkit-animation: scale 0.75s 0.36s infinite cubic-bezier(.2, .68, .18, 1.08);
          animation: scale 0.75s 0.36s infinite cubic-bezier(.2, .68, .18, 1.08); }

.loader {
  margin-top: 20rpx;
}
@keyframes rotate {
  0% {
    -webkit-transform: rotate(0deg) scale(1);
            transform: rotate(0deg) scale(1); }

  50% {
    -webkit-transform: rotate(180deg) scale(0.6);
            transform: rotate(180deg) scale(0.6); }

  100% {
    -webkit-transform: rotate(360deg) scale(1);
            transform: rotate(360deg) scale(1); } }

@keyframes scale {
  30% {
    -webkit-transform: scale(0.3);
            transform: scale(0.3); }

  100% {
    -webkit-transform: scale(1);
            transform: scale(1); }
}


.ad-image-list2{
  display: flex;
}

.ad-style2-left{
  flex: 1;
  margin-right: 10rpx;
}
.ad-style2-right{
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.ad-style3-top-box{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-content: center;
}
.ad-style3-top{
  width: calc(50% - 6rpx);
}
.ad-style3-bottom-box{
  display: grid;
  grid-template-columns: calc(100% / 3 - 7rpx) calc(100% / 3 - 7rpx) calc(100% / 3 - 7rpx);
  justify-content: space-between;
  align-content: center;
}

.isAdvList{
  background: unset !important;
}

.ad-image-list-style3{
  background: #ffffff;
  border-radius: 12rpx;

}

.adv-image{
  border-radius: 8rpx;
}

.grid-list{
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  align-items: center;
  /* padding: 10rpx; */
  box-sizing: border-box;
}

.image-item {
  width: calc(33.33% - 12rpx);
  margin: 6rpx 6rpx;
}